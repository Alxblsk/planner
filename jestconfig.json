{
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "testRegex": ".*spec\\.ts$",
  "moduleFileExtensions": ["js", "ts", "tsx", "json"],
  "moduleNameMapper": {
    "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
  },
  "globals": {
    "ts-jest": {
      "diagnostics": {
        "ignoreCodes": [151001]
      }
    }
  }
}
